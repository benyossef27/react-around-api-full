{
  "extends": "airbnb-base",
  "rules": {
  "no-underscore-dangle":  ["error", { "allow": ["_id"] }],
  "comma-dangle": "off",
  "no-console": "off",
  "implicit-arrow-linebreak": "off",
  "function-paren-newline": "off",
  "linebreak-style": 0,
  "operator-linebreak": "off",
  "object-curly-newline": "off",
  "no-unused-vars": ["error", { "argsIgnorePattern": "next" }]


}
}
